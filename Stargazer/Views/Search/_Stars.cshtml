@model List<Star>

<table>
    @foreach (Star star in Model)
    {
        <tr>
            <td>@Html.ActionLink(@star.name, "GetStarDetails", "Details", new { name = star.name, magnitude = star.magnitude, declination = star.declination, rightAscension = star.rightAscension }, new { })</td>
        </tr>
    }
</table>
