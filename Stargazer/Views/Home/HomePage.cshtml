@using Stargazer.Models
@model List<Event>
@{
    ViewBag.Title = "HomePage";
}

<h2>
    HomePage
</h2>
<img src="@(ViewBag.picUrl)" alt="Alternate Text" />

<h3>Events</h3>
<table>
    <tr>    
        @foreach(Event newEvent in Model)
        {
            <td>@Html.ActionLink(newEvent.Date.ToString(), "EventDetails", new {Id = newEvent.Id})</td>
        }
    </tr>
</table>

